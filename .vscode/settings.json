{
  "files.associations": {
    "crontab": "crontab",
    "LICENSE": "plaintext",
    "LICENSE.md": "markdown"
  },
  "shellformat.useEditorConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[bats]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[hosts]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  }
}
